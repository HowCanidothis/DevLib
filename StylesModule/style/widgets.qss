QPushButton#BtnLocked {
    min-width: 16px;
    min-height: 16px;
    max-width: 16px;
    max-height: 16px;
    padding: 0px;
}

QPushButton#BtnLocked:hover {
    background: none;
}

QPushButton#BtnLocked:checked {
    image: url("@cPath/lock.svg");
    background: none;
}

QPushButton#BtnLocked:!checked {
    image: url("@cPath/lock_open.svg");
}

QPushButton#BtnLocked:checked:disabled {
    image: url("@cPath/lock_disabled.svg");
}

